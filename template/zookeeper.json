  "disk_quota": ["1"],
  "gpu_quota": ["0"],
  "properties": {
    "ZOO_TICK_TIME": {
      "default": "4000",
      "required": true,
      "description": ""
    },
    "ZOO_INIT_LIMIT": {
      "default": "10",
      "required": true,
      "description": ""
    },
    "ZOO_SYNC_LIMIT": {
      "default": "5",
      "required": true,
      "description": ""
    },
    "ZOO_JVM_XMS": {
      "default": "512",
      "required": true,
      "description": ""
    },
    "ZOO_JVM_XMX": {
      "default": "512",
      "required": true,
      "description": ""
    }
  }
}